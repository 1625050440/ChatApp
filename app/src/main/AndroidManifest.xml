<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.enpit_p13.chatapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_kangaroo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- トップページのアクティビティ -->
        <activity
            android:name=".toppage.TopPageActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".messages.LatestMessagesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".toppage.TopPageActivity" />
        </activity>
        <activity android:name=".registerlogin.RegisterActivity" />
        <activity android:name=".registerlogin.Login" />
        <activity android:name=".messages.NewMessageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessagesActivity" />
        </activity>
        <activity android:name=".room_chat.Room_chat_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessagesActivity" />
        </activity>
        <activity android:name=".room_chat.Room_chat_from_ListView">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessagesActivity" />
        </activity>
        <activity android:name=".Activity_chat">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessagesActivity" />
        </activity>
        <activity android:name=".quetion.QuestiontempActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessagesActivity" />
        </activity>

        <!-- 自己分析シートのアクティビティ -->
        <activity
            android:name=".analyzesheet.OwnAnalysisActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".quetion.QuestiontempActivity_from_chat_all" />
        <activity android:name=".quetion.RoomIntroduceActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessagesActivity" />
        </activity>
        <activity android:name=".analyzesheet.SelectOwnAnalyzeActivity" />
        <activity
            android:name=".LookBackAnalyzeActivity"
            android:label="@string/title_activity_look_back_analyze"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>